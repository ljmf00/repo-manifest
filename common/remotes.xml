<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<!-- Github service provider -->
	<remote name="github"
		fetch="https://github.com"
		pushurl="ssh://git@github.com"
		alias="origin" />
	<remote name="github-private"
		fetch="ssh://git@github.com"
		pushurl="ssh://git@github.com"
		alias="origin" />

	<!-- Gitlab service provider -->
	<remote name="gitlab"
		fetch="https://gitlab.com"
		pushurl="ssh://git@gitlab.com"
		alias="origin" />

	<!-- Bitbucket service provider -->
	<remote name="bitbucket"
		fetch="https://bitbucket.org"
		pushurl="ssh://git@bitbucket.org"
		alias="origin" />

	<!-- GNU GCC Git server -->
	<remote name="gcc"
		fetch="https://gcc.gnu.org/git"
		pushurl="ssh://gcc.gnu.org/git"
		alias="origin" />

	<!-- AUR Archlinux Git server -->
	<remote name="archlinux-aur"
		fetch="https://aur.archlinux.org"
		pushurl="ssh://aur@aur.archlinux.org"
		alias="origin" />
</manifest>
